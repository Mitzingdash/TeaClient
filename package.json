{
  "name": "TeaClient",
  "version": "1.0.0",
  "description": "A Minecraft client for all",
  "main": "main.js",
  "repository": "https://github.com/Mitzingdash/TeaClient.git",
  "author": "Teaclient More info In app/licences/license",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "watch": "nodemon --exec electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "TeaClient",
    "dmg": {
      "title": "${productName} ${version}"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "axios": "^1.4.0",
    "electron": "^25.1.1",
    "electron-builder": "^24.4.0",
    "eslint": "^8.42.0",
    "nodemon": "^2.0.22"
  }
}
